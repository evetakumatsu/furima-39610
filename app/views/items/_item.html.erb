<li class='list'>
  <%= link_to '#' do %>
  <%= image_tag item.image, class: "item-img" %>
  <div class='item-info'>
    <h3 class='item-name'>
      <%= item.item_name %>
    </h3>
    <div class='item-price'>
      <span> <%= item.price %>å†† <br><%=  DeliveryCharge.find(item.delivery_charge_id).name %></span>
      <div class='star-btn'>
        <%= image_tag "star.png", class:"star-icon" %>
        <span class='star-count'>0</span>
      </div>
    </div>
  </div>
  <% end %>
</li>

